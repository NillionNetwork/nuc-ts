{
  "name": "@nillion/nuc",
  "version": "0.2.0-rc.0",
  "license": "MIT",
  "repository": "https://github.com/NillionNetwork/nuc-ts",
  "engines": {
    "node": ">=23"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/lib.d.ts",
      "import": "./dist/lib.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "fix": "pnpm exec biome check --fix --unsafe",
    "typecheck": "tsc --noEmit",
    "lint": "pnpm exec biome lint  --fix --unsafe",
    "format": "pnpm exec biome format --fix",
    "test": "vitest --run",
    "build:proto": "npx buf generate",
    "build": "pnpm build:proto && tsup",
    "install-hooks": "lefthook install",
    "attw": "attw --profile esm-only --pack ."
  },
  "peerDependencies": {
    "pino": "^9.7.0",
    "pino-pretty": "^13.0.0"
  },
  "dependencies": {
    "@bufbuild/protobuf": "^2.9.0",
    "@cosmjs/proto-signing": "^0.36.0",
    "@cosmjs/stargate": "^0.36.0",
    "@noble/curves": "^2.0.0",
    "@noble/hashes": "^2.0.0",
    "@scure/base": "^2.0.0",
    "es-toolkit": "^1.39.10",
    "ethers": "^6.15.0",
    "http-status-codes": "^2.3.0",
    "ky": "^1.10.0",
    "multiformats": "^13.4.1",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.18.2",
    "@biomejs/biome": "2.2.4",
    "@bufbuild/buf": "^1.57.2",
    "@bufbuild/protoc-gen-es": "^2.9.0",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@commitlint/types": "^19.8.1",
    "@keplr-wallet/types": "^0.12.274",
    "@types/debug": "^4.1.12",
    "@types/node": "^24.5.2",
    "@types/semver": "^7.7.1",
    "@vitest/coverage-v8": "^3.2.4",
    "docker-compose": "^1.3.0",
    "dotenv": "^17.2.2",
    "fast-check": "^4.3.0",
    "lefthook": "^1.13.2",
    "semver": "^7.7.2",
    "tsup": "^8.5.0",
    "type-fest": "^5.0.1",
    "typedoc": "^0.28.13",
    "typescript": "^5.9.2",
    "vite": "^7.1.7",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "packageManager": "pnpm@10.17.0",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ]
}
