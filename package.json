{
  "name": "@nillion/nuc",
  "version": "1.0.0-rc.4",
  "license": "MIT",
  "repository": "https://github.com/NillionNetwork/nuc-ts",
  "engines": {
    "node": ">=23"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/lib.d.ts",
      "import": "./dist/lib.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "fix": "pnpm exec biome check --fix --unsafe && tsc --noEmit",
    "fmt": "pnpm exec biome format --fix",
