    "@noble/curves": "^2.0.1",
    "@noble/hashes": "^2.0.1",
    "@scure/base": "^2.0.0",
    "es-toolkit": "^1.39.10",
    "ethers": "^6.15.0",
    "http-status-codes": "^2.3.0",
    "ky": "^1.11.0",
    "multiformats": "^13.4.1",
    "viem": "^2.37.13",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.18.2",
    "@biomejs/biome": "2.2.4",
    "@bufbuild/buf": "^1.57.2",
