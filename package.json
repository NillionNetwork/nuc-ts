    "@types/node": "^24.7.0",
    "@types/semver": "^7.7.1",
    "@vitest/coverage-v8": "^3.2.4",
    "docker-compose": "^1.3.0",
    "dotenv": "^17.2.3",
    "fast-check": "^4.3.0",
    "lefthook": "^1.13.6",
    "semver": "^7.7.2",
    "tsup": "^8.5.0",
    "tsx": "^4.20.6",
    "type-fest": "^5.0.1",
    "typedoc": "^0.28.13",
    "typescript": "^5.9.3",
    "vite": "^7.1.9",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
